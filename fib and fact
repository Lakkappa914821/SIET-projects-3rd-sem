def factorial(n):
  if n == 1:
    return 1
  else:
    return n*factorial(n-1)

# fibonacci series: sum of previous two numbers

def fib(n):
  if n<=1:
    return n
  else:
    return fib(n-1)+fib(n-2)
term=int(input("enter the number of terms: "))
if term <=0:
  print("enter the positive number")
else:
  print("fibonacci series")
  for i in range(term):
    print(fib(i))
